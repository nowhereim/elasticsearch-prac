  <script>
    const socket = io("localhost:3000");
    socket.on("tryAgain", (data) => {
      console.log(data);
      trynew(data);
    });

    const trynew = (data) => {
      $.ajax({
        url: data.url,
        type: "POST",
        contentType: "application/json;charset=utf-8",
        data: JSON.stringify({
          start: data.startNum,
          word: data.word,
        }),
        success: function (data) {
          console.log(data);
        },
      });
    };
  </script>
